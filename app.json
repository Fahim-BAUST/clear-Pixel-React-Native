{
  "expo": {
    "name": "clear-pixel-android",
    "slug": "clear-pixel-android",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "https://i.ibb.co/Cm6dM6x/boy-girl-with-camera-white-background-1308-41950.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.fanhim.clearpixelandroid"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}